  <rules>
    <rule name="icon-alternates-escalator">
      <conditionset>
        <condition minimum="650" maximum="700" name="Weight" />
        <sub name="ico.escalator" with="ico.escalator.bold" />
      </conditionset>
    </rule>
    <rule name="icon-alternates-disabled">
      <conditionset>
        <condition minimum="450" maximum="700" name="Weight" />
        <sub name="ico.disabled" with="ico.disabled.bold" />
      </conditionset>
    </rule>
  </rules>